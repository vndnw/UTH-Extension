{
  "manifest_version": 3,
  "name": "UTH - Xem học phần đã đăng ký",
  "version": "1.0",
  "description": "Xem học phần đã đăng ký trong học kỳ",
  "permissions": ["scripting"],
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png",
    "48": "icons/favicon-32x32.png",
    "128": "icons/android-chrome-192x192.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Xem học phần đã đăng ký",
    "default_icon": {
      "16": "icons/favicon-16x16.png",
      "32": "icons/favicon-32x32.png",
      "48": "icons/favicon-32x32.png",
      "128": "icons/android-chrome-192x192.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
